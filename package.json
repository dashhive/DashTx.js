{
  "name": "dashtx",
  "version": "0.13.0",
  "description": "Create DASH Transactions with Vanilla JS (0 deps, cross-platform)",
  "main": "dashtx.js",
  "files": [
    "index.js",
    "dashtx.js",
    "./lib/"
  ],
  "browser": {
    "index.js": "dashtx.js",
    "crypto": false
  },
  "bin": {
    "dashtx-inspect": "./bin/inspect.js"
  },
  "scripts": {
    "bump": "npm version -m \"chore(release): bump to v%s\"",
    "fmt": "npx -p prettier@2.x -- prettier -w '**/*.{js,md}'",
    "lint": "npx -p typescript@4.x -- tsc -p ./jsconfig.json",
    "prepublish": "npx -p jswt@1.x -- reexport",
    "test": "npx -p pta@1.x -- pta 'tests/**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dashhive/dashtx.js.git"
  },
  "keywords": [],
