  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/dashhive/dashtx.js/issues"
  },
  "homepage": "https://github.com/dashhive/dashtx.js#readme",
  "devDependencies": {
    "@dashincubator/secp256k1": "^1.7.1-5",
    "dashkeys": "^1.0.1",
    "zora": "^5.2.0"
  }
}
